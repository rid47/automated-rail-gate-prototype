#include <Servo.h>

Servo servo;
Servo servo2;
Servo servo3;
Servo servo4;

void setup() {

servo.attach(15);//D5
servo2.attach(14);//D6
servo3.attach(13);//D7
servo4.attach(12);//D8


servo.write(0);
servo2.write(0);
servo3.write(0);
servo4.write(0);


delay(2000);

}

void loop() {

servo.write(150);
servo2.write(150);
servo3.write(150);
servo4.write(150);
delay(1000);

servo.write(0);
servo2.write(0);
servo3.write(0);
servo4.write(0);
delay(1000);

}
